{
  "services": {
    "web-server": {
      "image": "nginx:latest",
      "pullPolicy": "missing",
      "namePrefix": "nginx-proxy",
      "baseName": "<base-container-name>",
      "currentSuffix": "<optional-container-name-suffix>",
      "previousSuffix": "<optional-demoted-container-name-suffix>",
      "ports": [
        {
          "target": "80"
        },
        {
          "target": "8443",
          "published": "443"
        }
      ],
      "volumes": [
        {
          "type": "bind",
          "source": "/opt/nginx/data",
          "target": "/data",
          "readOnly": true,
          "bind": {
            "createHostPath": true
          }
        },
        {
          "type": "bind",
          "source": "/opt/nginx/ssl",
          "target": "/ssl",
          "readOnly": true
        },
        {
          "type": "volume",
          "source": "nginx-config",
          "target": "/etc/nginx"
        }
      ],
      "envVars": {
        "TZ": "UTC",
        "APP_ENV": "PROD"
      },
      "networks": {
        "shared-services-network": {
          "aliases": [
            "web-proxy"
          ]
        },
        "private-network": {}
      },
      "hostname": "sys--nginx",
      "extraHosts": {
        "host.docker.internal": "host-gateway"
      },
      "dns": [
        "8.8.8.8",
        "8.8.4.4"
      ],
      "restart": "unless-stopped",
      "dockerCommandOptions": [
        "-d",
        "--memory 256MB"
      ]
    },
    "cache": {
      "image": "redis/redis-stack",
      "pullPolicy": "always",
      "name_prefix": "db--",
      "base_name": "redis-cache",
      "current_suffix": "-current",
      "previous_suffix": "-previous",
      "ports": [
        {
          "target": "6379",
          "published": "6379",
          "protocol": "tcp"
        }
      ],
      "volumes": [
        {
          "type": "volume",
          "source": "redis-cache-data",
          "target": "/data",
          "volume": {
            "nocopy": true
          },
          "skip_backup": false
        }
      ],
      "envVars": {
        "TZ": "UTC"
      },
      "networks": {
        "private-network": {
          "ipV4Address": "172.20.1.10",
          "aliases": [
            "redis-cache"
          ]
        }
      },
      "restart": "always",
      "dockerCommandOptions": [
        "-d",
        "--memory 1GB"
      ]
    }
  },
  "networks": {
    "shared-services-network": {
      "name": "shared-services-net",
      "attachable": true,
      "driver": "bridge"
    },
    "private-network": {
      "name": "private-net",
      "attachable": true,
      "driver": "bridge",
      "ipam": {
        "config": {
          "subnet": "172.20.1.1/24",
          "ipRange": "172.20.1.1/26"
        }
      },
      "external": true
    }
  }
}